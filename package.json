{
	"name": "@fdsouvenir/falcon-dash",
	"version": "0.12.0",
	"description": "Web dashboard for the OpenClaw AI platform",
	"repository": {
		"type": "git",
		"url": "https://github.com/fdsouvenir/falcon-dash.git"
	},
	"homepage": "https://github.com/fdsouvenir/falcon-dash#readme",
	"bugs": {
		"url": "https://github.com/fdsouvenir/falcon-dash/issues"
	},
	"author": "fdsouvenir",
	"license": "CC-BY-NC-ND-4.0",
	"keywords": [
		"openclaw",
		"dashboard",
		"ai",
		"sveltekit",
		"websocket"
	],
	"type": "module",
	"bin": {
		"falcon-dash": "./bin/falcon-dash.js"
	},
	"files": [
		"bin/",
		"build/",
		"src/",
		"static/",
		"svelte.config.js",
		"vite.config.ts",
		"tsconfig.json",
		"package.json"
	],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"analyze": "ANALYZE=true vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui",
		"prepare": "husky",
		"prepublishOnly": "npm run build",
		"release": "npm version"
	},
	"lint-staged": {
		"*.{js,ts,svelte}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md,css,html,yaml,yml}": [
			"prettier --write"
		]
	},
	"devDependencies": {
		"@eslint/js": "^10.0.0",
		"@sentry/vite-plugin": "^4.9.1",
		"@types/better-sqlite3": "^7.6.13",
		"@types/katex": "^0.16.8",
		"@types/node": "^25.3.0",
		"@vitest/coverage-v8": "^4.0.18",
		"@vitest/ui": "^4.0.18",
		"eslint": "^10.0.0",
		"eslint-config-prettier": "^10.0.0",
		"eslint-plugin-svelte": "^3.15.0",
		"globals": "^17.3.0",
		"happy-dom": "^20.7.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.4.0",
		"prettier-plugin-svelte": "^3.3.0",
		"rollup-plugin-visualizer": "^7.0.0",
		"typescript-eslint": "^8.0.0",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"@internationalized/date": "^3.11.0",
		"@lucide/svelte": "^0.575.0",
		"@sentry/sveltekit": "^10.39.0",
		"@sveltejs/adapter-node": "^5.5.3",
		"@sveltejs/kit": "^2.15.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/ws": "^8.18.1",
		"@xterm/addon-fit": "^0.11.0",
		"@xterm/addon-web-links": "^0.12.0",
		"@xterm/xterm": "^6.0.0",
		"better-sqlite3": "^12.6.2",
		"bits-ui": "^2.16.2",
		"clsx": "^2.1.1",
		"katex": "^0.16.28",
		"mermaid": "^11.12.2",
		"node-pty": "^1.1.0",
		"rehype-katex": "^7.0.1",
		"rehype-raw": "^7.0.0",
		"rehype-sanitize": "^6.0.0",
		"rehype-stringify": "^10.0.1",
		"remark-gfm": "^4.0.1",
		"remark-math": "^6.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"runed": "^0.37.1",
		"shiki": "^3.22.0",
		"svelte": "^5.53.0",
		"svelte-check": "^4.4.3",
		"svelte-gestures": "^5.2.2",
		"svelte-toolbelt": "^0.10.6",
		"tailwind-merge": "^3.5.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.2.1",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.7.0",
		"unified": "^11.0.5",
		"vite": "^7.0.0",
		"web-vitals": "^5.1.0",
		"ws": "^8.19.0"
	}
}
